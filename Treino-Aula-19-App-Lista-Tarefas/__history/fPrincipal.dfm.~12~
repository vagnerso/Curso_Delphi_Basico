      ReadOnly = True
      TabOrder = 1
      ExplicitWidth = 268
      ExplicitHeight = 47
    end
  end
  object dtsTarefas: TDataSource
    DataSet = dtmPrincipal.memTarefas
    Left = 720
    Top = 16
  end
end
